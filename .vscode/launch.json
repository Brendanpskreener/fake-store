  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Create User",
      "program": "${workspaceFolder}/node_modules/.bin/serverless",
      "args": [
        "invoke",
        "local",
        "-f",
        "putUser",
        "-p",
        "__dev__/local-events/putUser.event.js"
      ],
      "console": "integratedTerminal"
    }
  ]
}